{
	"compilerOptions": {
		"sourceMap": false,
		"outDir": "./dist",
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"strict": false,
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"declaration": true,
		"declarationMap": false,
		"declarationDir": "./dist/types",
		"downlevelIteration": true,
		"emitDeclarationOnly": true,
		"removeComments": true,
		"preserveConstEnums": true,
	},
	"include": [
		"src"
	]
}